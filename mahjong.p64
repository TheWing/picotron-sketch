function pill(x,y,w,h,d,col)
	local r=d
	color(col)
	if r>0 then 
		rectfill(x+r,y,x+w-r,y+h)
		rectfill(x,y+r,x+w,y+h-r)
		circfill(x+r,y+r,r)
		circfill(x+w-r,y+r,r)
		circfill(x+r,y+h-r,r)
		circfill(x+w-r,y+h-r,r)
	else
		rectfill(x,y,x+w,y+h)
	end
end

function rline(x1,y1,ox1,oy1,ox2,oy2,col,rot)
	local x,y,xx,yy=x1+ox1,y1+oy1,x1+ox2,y1+oy2
	rot=rot or 0
	if rot==1 then 
		x,y=x1+oy1,y1-ox1
		xx,yy=x1+oy2,y1-ox2
	elseif rot==3 then 
		x,y=x1-oy1,y1+ox1
		xx,yy=x1-oy2,y1+ox2
	end
	line(x,y,xx,yy,col)
end

function rpset(xx,yy,ox,oy,col,rot)
	local x,y=xx+ox,yy+oy
	rot=rot or 0
	if rot==1 then 
		x,y=xx+oy,yy-ox
	elseif rot==3 then 
		x,y=xx-oy,yy+ox
	end
	pset(x,y,col)
end

