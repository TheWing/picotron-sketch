	elseif rot==3 then 
		x,y=xx+oy,yy+ox
	end
	local i=0
	--bird??
	rline(x,y,-w,0,w+1,0,GREEN,rot)
	rline(x,y,-w,2,w+1,2,GREEN,rot)
	rpset(x,y,-w,1,GREEN,rot)
	rpset(x,y,w+1,1,GREEN,rot)
	for i=0,6,2 do
		rline(x,y,w/20*i-w/4,0,-w/8+w/8*i,h/1.5+h/20*(6-i),BLUE,rot)
		rline(x,y,w/12*i-w/3,0,w/6+w/12*i,-h/3,GREEN,rot)
		rline(x,y,w/10*i,-h/1.8,w/6+w/12*i,-h/3,GREEN,rot)
		rline(x,y,w/10*i,-h/1.8,w/4,-h/1.1,RED,rot)
		rline(x,y,0,-w/10*i,-w/1.2,-w/5*i,BLUE,rot)
	end
	rcircfill(x,y,w/3,-h/2,(w/h)*w/1.4,WHITE,rot)
	rcirc(x,y,w/3,-h/2,(w/h)*w/1.4,RED,rot)
	rpset(x,y,w/3,-h/2,RED,rot)
end

function bam_c(xx,yy,ox,oy,w,h,a,col,rot)
	local x,y=xx+ox,yy+oy
	rot=rot or 0
	if rot==1 then 
		x,y=xx+oy,yy+ox
		w,h=h,w
	elseif rot==3 then 
		x,y=xx+oy,yy+ox
		w,h=h,w
	end
	local ma,mi=max(w,h),min(w,h)
	ma=ma*0.8
	mi=mi*0.8
	color(col)
	if a==1 then
		line(x+ma-mi,y-ma-mi,x-ma-mi,y+ma-mi)
		line(x+ma+mi,y-ma+mi,x-ma+mi,y+ma+mi)
		line(x+ma-mi,y-ma-mi,x+ma+mi,y-ma+mi)
		line(x-ma-mi,y+ma-mi,x-ma+mi,y+ma+mi)
		line(x-mi,y-mi,x+mi,y+mi)
	elseif a==2 then
		line(x-ma+mi,y-ma-mi,x+ma+mi,y+ma-mi)
		line(x-ma-mi,y-ma+mi,x+ma-mi,y+ma+mi)
		line(x-ma+mi,y-ma-mi,x-ma-mi,y-ma+mi)
		line(x+ma+mi,y+ma-mi,x+ma-mi,y+ma+mi)
		line(x+mi,y-mi,x-mi,y+mi)
	else
		x1=0-w
		y1=0-h
		x2=0-w
		y2=0+h
		--rectfill(x+x1,y+y1,x-x2,y+y2,WHITE)
		color(col)
		line(x-w,y-h,x-w,y+h)
		line(x+w,y-h,x+w,y+h)
		line(x+x1,y+y1,x-x1,y+y1)
		line(x+x2,y+y2,x-x2,y+y2)
		line(x+x1,y,x-x1,y)
		line(x+x2,y,x-x2,y)
	end
end

function bam(x,y,w,h,num,rot)
	if num>0 and num<10 then
		--pill(x,y,w,h,RADIUS,WHITE) --base
	end
	if num==1 then
		bam_o(x,y,w/2,h/2,w/2.5,h/2,rot)
	elseif num==2 then
		bam_c(x,y,w/2,h/4  ,w/15,h/6,0,BLUE,rot)
		bam_c(x,y,w/2,h/4*3,w/15,h/6,0,GREEN,rot)
	elseif num==3 then
		bam_c(x,y,w/2     ,h/4  ,w/15,h/6,0,BLUE,rot)
		bam_c(x,y,w/4+w/16,h/4*3,w/15,h/6,0,GREEN,rot)
		bam_c(x,y,w/4*3   ,h/4*3,w/15,h/6,0,GREEN,rot)
	elseif num==4 then
		bam_c(x,y,w/4+w/16,h/4  ,w/15,h/6,0,GREEN,rot)
		bam_c(x,y,w/4*3   ,h/4  ,w/15,h/6,0,BLUE,rot)
		bam_c(x,y,w/4+w/16,h/4*3,w/15,h/6,0,BLUE,rot)
		bam_c(x,y,w/4*3   ,h/4*3,w/15,h/6,0,GREEN,rot)
	elseif num==5 then
		bam_c(x,y,w/4  ,h/4  ,w/15,h/6,0,GREEN,rot)
		bam_c(x,y,w/4*3,h/4  ,w/15,h/6,0,BLUE,rot)
		bam_c(x,y,w/4  ,h/4*3,w/15,h/6,0,BLUE,rot)
		bam_c(x,y,w/4*3,h/4*3,w/15,h/6,0,GREEN,rot)
		bam_c(x,y,w/2  ,h/2  ,w/15,h/6,0,RED,rot)
	elseif num==6 then
		bam_c(x,y,w/5+w/15    ,h/4   ,w/15,h/6,0,BLUE,rot)
		bam_c(x,y,w/5*2.5+w/20,h/4   ,w/15,h/6,0,BLUE,rot)
		bam_c(x,y,w/5*4       ,h/4   ,w/15,h/6,0,BLUE,rot)
		bam_c(x,y,w/5+w/15    ,h/4*3,w/15,h/6,0,GREEN,rot)
		bam_c(x,y,w/5*2.5+w/20,h/4*3,w/15,h/6,0,GREEN,rot)
		bam_c(x,y,w/5*4       ,h/4*3,w/15,h/6,0,GREEN,rot)
	elseif num==7 then
		bam_c(x,y,w/5*2.5+w/20,h/6  ,w/15,h/8,0,RED,rot)
		bam_c(x,y,w/5+w/15    ,h/6*3,w/15,h/8,0,GREEN,rot)
		bam_c(x,y,w/5*2.5+w/20,h/6*3,w/15,h/8,0,BLUE,rot)
		bam_c(x,y,w/5*4       ,h/6*3,w/15,h/8,0,GREEN,rot)
		bam_c(x,y,w/5+w/15    ,h/6*5,w/15,h/8,0,GREEN,rot)
		bam_c(x,y,w/5*2.5+w/20,h/6*5,w/15,h/8,0,BLUE,rot)
		bam_c(x,y,w/5*4       ,h/6*5,w/15,h/8,0,GREEN,rot)
	elseif num==8 then
		bam_c(x,y,w/4+w/12  ,h/4+h/28  ,w/15,h/6,1,BLUE,rot)
		bam_c(x,y,w/4*3-w/12,h/4+h/28  ,w/15,h/6,2,BLUE,rot)
		bam_c(x,y,w/4+w/12  ,h/4*3-h/28,w/15,h/6,2,GREEN,rot)
		bam_c(x,y,w/4*3-w/12,h/4*3-h/28,w/15,h/6,1,GREEN,rot)
		bam_c(x,y,w/4       ,h/4+h/28  ,w/15,h/6,0,BLUE,rot)
		bam_c(x,y,w/4*3+w/16,h/4+h/28  ,w/15,h/6,0,BLUE,rot)
		bam_c(x,y,w/4       ,h/4*3-h/28,w/15,h/6,0,GREEN,rot)
		bam_c(x,y,w/4*3+w/16,h/4*3-h/28,w/15,h/6,0,GREEN,rot)
	elseif num==9 then
		bam_c(x,y,w/5+w/15    ,h/6,w/15,h/8,0,GREEN,rot)
		bam_c(x,y,w/5*2.5+w/20,h/6,w/15,h/8,0,RED,rot)
		bam_c(x,y,w/5*4       ,h/6,w/15,h/8,0,BLUE,rot)
		bam_c(x,y,w/5+w/15    ,h/6*3,w/15,h/8,0,GREEN,rot)
		bam_c(x,y,w/5*2.5+w/20,h/6*3,w/15,h/8,0,RED,rot)
		bam_c(x,y,w/5*4       ,h/6*3,w/15,h/8,0,BLUE,rot)
		bam_c(x,y,w/5+w/15    ,h/6*5,w/15,h/8,0,GREEN,rot)
		bam_c(x,y,w/5*2.5+w/20,h/6*5,w/15,h/8,0,RED,rot)
		bam_c(x,y,w/5*4       ,h/6*5,w/15,h/8,0,BLUE,rot)
	end
end



------- MAN -------

function man_b(xx,yy,ox,oy,w,h,rot)
	oy=oy-h/2
	local x,y=xx+ox,yy+oy
	rot=rot or 0
	if rot==1 then 
		x,y=xx+oy,yy+ox
	end
	if rot==3 then 
		x,y=xx-oy,yy+ox
	end
	--character
	--rrect(x,y,-w,-h,w,h,RED,rot)
	rline(x,y,-w/1.5,-h/2,-1,-h/2,RED,rot)
	rline(x,y,w/1.5+1,-h/2,1,-h/2,RED,rot)
	rline(x,y,-w/3,-h/1.8,-w/3,-h/3,RED,rot)
	rline(x,y,w/3+1,-h/1.8,w/3+1,-h/3,RED,rot)
	rline(x,y,0,-h/6,0,h/4*4,RED,rot)
	rline(x,y,-w/1.8,h/4*3.5,w/1.8+1,h/4*3.5,RED,rot)
	rline(x,y,-w/1.8,h/4*3.5,-w/1.8,h/4*5.5,RED,rot)
	rline(x,y,w/1.8+1,h/4*3.5,w/1.8+1,h/4*5.5,RED,rot)
	rline(x,y,w/1.8-1,h/4*5.5,w/1.8+1,h/4*5.5,RED,rot)
	rline(x,y,-w/3,h/4*5.5,w/3,h/4*4,RED,rot)
	rline(x,y,w/2,h/4*5.5,w/3,h/4*4,RED,rot)
	rrect(x,y,-w/2,-h/6,w/2,h/4,RED,rot)
	rrect(x,y,-w/2,h/4,w/2,h/4*2,RED,rot)
end


function man(x,y,w,h,num,rot)
	if rot%2==0 then 
		x=x+w/2
		y=y+h/4
		w=w/2
		h=h/4
	elseif rot==1 then 
		x=x+h/4
		y=y+w/2
		w=w/2
		h=h/4
	elseif rot==3 then 
		x=x-h/4+h
		y=y+w/2
		w=w/2
		h=h/4
	end
	if num>0 and num<10 then
		--pill(x,y,w,h,RADIUS,WHITE) --base
		man_b(x,y,0,h/1*2-1,w,h*0.8,rot)
	end
	if num==1 then
		rline(x,y,-w/2,0,w/2,0,BLUE,rot)
		rline(x,y,-w/2,1,w/2,-1,BLUE,rot)
	elseif num==2 then
		rline(x,y,-w/2,h/3,w/2,h/3,BLUE,rot)
		rline(x,y,-w/2,1+h/3,w/2,-1+h/3,BLUE,rot)
		rline(x,y,-w/3,-h/3,w/3,-h/3,BLUE,rot)
		rline(x,y,-w/3,1-h/3,w/3,-1-h/3,BLUE,rot)
	elseif num==3 then
		rline(x,y,-w/2,h/2,w/2,h/2,BLUE,rot)
		rline(x,y,-w/2,1+h/2,w/2,-1+h/2,BLUE,rot)
		rline(x,y,-w/3,0,w/3,0,BLUE,rot)
		rline(x,y,-w/3,1,w/3,-1,BLUE,rot)
		rline(x,y,-w/4,-h/2,w/3,-h/2,BLUE,rot)
		rline(x,y,-w/4,1-h/2,w/3,-1-h/2,BLUE,rot)
	elseif num==4 then
		rrect(x,y,-w/2,-h/3,w/2,h/3,BLUE,rot)
		rline(x,y,-w/5,-h/3,-w/8,h/3,BLUE,rot)
		rline(x,y,w/4,-h/3,w/5,h/3,BLUE,rot)
		rline(x,y,0,h/3,w/2,h/3+1,BLUE,rot)
		rpset(x,y,-w/2-1,-h/3,BLUE,rot)
	elseif num==5 then
		rline(x,y,-w/2,0,-w/8,-h/2,BLUE,rot)
		rline(x,y,-w/2,0,-w/5,-h/2,BLUE,rot)
		rline(x,y,w/3,-h/2,0,h/3,BLUE,rot)
		rline(x,y,w/3,-h/2,w/8,-h/2,BLUE,rot)
		rline(x,y,-w/3,h/3,w/2,h/3,BLUE,rot)
		rline(x,y,-w/3,h/2,-w/3,0,BLUE,rot)
		rline(x,y,-w/8,0,w/2.5,0,BLUE,rot)
		rline(x,y,w/2.5,0,w/5,h/3,BLUE,rot)
	elseif num==6 then
		rline(x,y,0,-h/2,0,-h/4,BLUE,rot)
		rline(x,y,-w/2,-h/8,w/2,-h/4,BLUE,rot)
		rline(x,y,w/4,h/8,w/2,h/4,BLUE,rot)
		rline(x,y,-w/4,h/4,-w/2,h/3,BLUE,rot)
	elseif num==7 then
		rline(x,y,-w/8,-h/2,-w/8,h/3,BLUE,rot)
		rline(x,y,-w/1.8,h/5,0,-h/8,BLUE,rot)
		rline(x,y,0,-h/8,w/2,-h/8,BLUE,rot)
		rline(x,y,w/2.5,h/2,-w/8,h/3,BLUE,rot)
	elseif num==8 then
		rline(x,y,-w/10,-h/2,w/8,h/8,BLUE,rot)
		rline(x,y,w/1.5,h/2,w/8,h/8,BLUE,rot)
		rline(x,y,-w/1.7,h/3,-w/3,-h/8,BLUE,rot)
	elseif num==9 then
		rline(x,y,-w/2,-h/8,w/4,-h/8,BLUE,rot)
		rline(x,y,w/4,-h/8,0,h/2,BLUE,rot)
		rline(x,y,w/2,h/2.5,0,h/2,BLUE,rot)
		rline(x,y,-w/1.7,h/2,0,-h/2,BLUE,rot)
	end
end


------- HONORS -------
-- 1-4 winds
-- 5-7 dragons


function hon(x,y,w,h,num,rot)
	if num>0 and num<8 then
		--pill(x,y,w,h,RADIUS,WHITE) --base
	end
	if num<5 then
		y=y+1
	end
	if rot%2==0 then 
		x=x+w/2
		y=y+h/2
		w=w/1.2
		h=h/1.2
	else
		x=x+h/2
		y=y+w/2
		w=w/1.2
		h=h/1.2
	end
	if num==1 then 			-- East
		rline(x,y,0,-h/2.5,0,h/3,BLUE,rot)
		rline(x,y,-w/8,h/4,0,h/3,BLUE,rot)
		rline(x,y,0,-h/20,w/3,h/5,BLUE,rot)
		rline(x,y,0,-h/20,-w/3,h/5,BLUE,rot)
		rrect(x,y,-w/3,-h/4,w/3+1,-h/20,BLUE,rot)
		rline(x,y,-w/8,-h/8,w/8+1,-h/8,BLUE,rot)
		rline(x,y,-w/8,-h/3,w/8+1,-h/3,BLUE,rot)
	elseif num==2 then 		-- North
		rline(x,y,0,-h/3,-1,-h/14,BLUE,rot)
		rline(x,y,-w/8,h/4,w/8+1,h/5,BLUE,rot)
		rline(x,y,0,h/20,w/5,-h/10,BLUE,rot)
		rline(x,y,0,h/20,-w/5,-h/10,BLUE,rot)
		rline(x,y,0,h/20,0,h/3,BLUE,rot)
		rline(x,y,-w/8,h/8,w/8+1,h/8,BLUE,rot)
		rline(x,y,-w/8,-h/5,w/8+1,-h/4,BLUE,rot)
		rline(x,y,-w/3,0,w/3+1,-h/20,BLUE,rot)
		rline(x,y,-w/3,0,-w/3,h/4,BLUE,rot)
		rline(x,y,w/3+1,-h/20,w/3+1,h/3,BLUE,rot)
		rline(x,y,w/3-1,h/3,w/3+1,h/3,BLUE,rot)
	elseif num==3 then 		-- West
		rline(x,y,-w/6,-h/4,w/6+1,-h/3.5,BLUE,rot)
		rrect(x,y,-w/3,0,w/3+1,h/3,BLUE,rot)
		rline(x,y,-w/8,-h/10,-w/8,h/5,BLUE,rot)
		rline(x,y,w/8,-h/10,w/8,h/6,BLUE,rot)
		rline(x,y,w/8,h/6,w/4,h/6,BLUE,rot)
	elseif num==4 then 		-- South
		h=h*1.3
		rline(x,y,-w/12,-h/8,-w/12,h/4,BLUE,rot)
		rline(x,y,w/4,-h/4,w/4,h/4,BLUE,rot)
		rline(x,y,w/2,h/4,w/4,h/4,BLUE,rot)
		rline(x,y,w/4,h/6,w/2,0,BLUE,rot)
		rline(x,y,-w/3,-h/8,-w/3,0,BLUE,rot)
		rline(x,y,-w/2.5,h/4,w/4,-h/8,BLUE,rot)
	elseif num==5 then		-- White dragon
	elseif num==6 then 		-- Green dragon
		w=w*1.1
		rline(x,y,-w/3+1-1,0,-w/10,-h/3,GREEN,rot)
		rline(x,y,w/2-1,0,w/10-1,-h/3,GREEN,rot)
		rline(x,y,-w/10,-h/3,-w/4,-h/3,GREEN,rot)
		rline(x,y,w/10-1,-h/3,w/4-1,-h/2.5,GREEN,rot)
		rline(x,y,w/12-1,-h/8,w/2.5-1,-h/3,GREEN,rot)
		rline(x,y,-w/5,-h/8,-w/3,-h/4,GREEN,rot)
		rline(x,y,-w/10-1,-h/20,-1,-h/20,GREEN,rot)
		rline(x,y,-w/10-1,h/4,-1,-h/20,GREEN,rot)
		rline(x,y,-w/10-1,h/4,-1,h/5,GREEN,rot)
		rline(x,y,-w/20-1,h/2.5,-1,h/5,GREEN,rot)
		rline(x,y,-w/20-1,h/2.5,-w/4-1,h/3,GREEN,rot)
		rline(x,y,-w/4-1,h/4,w/3-1,h/5,GREEN,rot)
		rline(x,y,w/8-1,h/2.5,w/3-1,h/5,GREEN,rot)
		rline(x,y,w/8-1,h/3,w/3-1,h/2.5,GREEN,rot)
		rline(x,y,w/6-1,0,w/6-1,h/10,GREEN,rot)
		rline(x,y,w/6*2-1,0,w/6*2-1,h/10,GREEN,rot)
		rline(x,y,w/6*2-1,0,w/6-1,0,GREEN,rot)
	elseif num==7 then 		-- Red dragon
		rline(x,y,-w/3,-h/4,w/3+1,-h/4,RED,rot)
		rline(x,y,-w/4,0,-w/3,-h/4,RED,rot)
		rline(x,y,w/4,0,w/3+1,-h/4,RED,rot)
		rline(x,y,-w/4,-1,w/4,-1,RED,rot)
		rline(x,y,0,-h/2,0,h/2.5,RED,rot)
	end
end

