function check_pon(t,n,p,m,b,w,d)
	if t==PIN then
		if p[n]>1 then
			return true
		end
	end
	if t==MAN then
		if m[n]>1 then
			return true
		end
	end
	if t==BAM then
		if b[n]>1 then
			return true
		end
	end
	if t==WIN then
		if w[n]>1 then
			return true
		end
	end
	if t==DRA then
		if d[n]>1 then
			return true
		end
	end
	return false
end

function check_chi(t,n,p,m,b,w,d)
	if t==PIN then
		return chic(n,p)
	end
	if t==MAN then
		return chic(n,m)
	end
	if t==BAM then
		return chic(n,b)
	end
	return false
end

function chic(n,p)
	if n>2 then 
		if p[n-1]>0 and p[n-2]>0 then
			return true
		end
	end
	if n<8 then
		if p[n+1]>0 and p[n+2]>0 then
			return true
		end
	end
	if n>1 and n<9 then
		if p[n-1]>0 and p[n+1]>0 then
			return true
		end
	end
	return false
end




