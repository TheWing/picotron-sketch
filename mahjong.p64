-- Pure one-suit 80p
	if suit==2 then p=p+80 end
-- Nine Gates 480p
	if suit==2 and nums[1]==3 and nums[9]==3 and nums[2]>0 and nums[3]>0 and nums[4]>0 and nums[5]>0 and nums[6]>0 and nums[7]>0 and nums[8]>0 then
